        <camunda:formData>
          <camunda:formField id="selBoxRecenzent" type="enum" />
        </camunda:formData>
        <camunda:taskListener event="create">
          <camunda:script scriptFormat="Groovy">taskFormData = task.getExecution().getProcessEngineServices().getFormService().getTaskFormData(task.getId());
recenzentsIds = new java.util.ArrayList();
formFields = taskFormData.getFormFields();
if(formFields!=null){
   for(field in formFields){
       if( field.getId() == "selBoxRecenzent" ){
           items = field.getType().getValues();
           for(r in recenzenti){
               items.put(r.getId(),r.getFirstName()+" "+r.getLastName());
           }
       }
   }
}</camunda:script>
        </camunda:taskListener>
        <camunda:taskListener event="complete">
          <camunda:script scriptFormat="Groovy">taskFormData = task.getExecution().getProcessEngineServices().getFormService().getTaskFormData(task.getId());

if(izabraniRecenzenti != null) {
 recenzentsIds = izabraniRecenzenti;
} else {
 recenzentsIds= new java.util.ArrayList();
}

formFields = taskFormData.getFormFields();
if(formFields!=null){
   for(field in formFields){
       if(field.getId() == "selBoxRecenzent"){
           if(field.getValue()!="none")
                recenzentsIds.add(field.getValue());
       }
   }
}
task.getExecution().setVariable("izabraniRecenzenti", recenzentsIds);</camunda:script>
        </camunda:taskListener>
